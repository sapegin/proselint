{
  "name": "proselint",
  "version": "0.0.0-development",
  "description": "Proselint wrapper with a friendly reporter",
  "author": {
    "name": "Artem Sapegin",
    "url": "http://sapegin.me"
  },
  "homepage": "https://github.com/sapegin/proselint",
  "repository": "sapegin/proselint",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "main": "src",
  "bin": {
    "proselintjs": "src/index.js"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "lint": "eslint . --ext .js --fix",
    "test": "npm run lint",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "dependencies": {
    "async": "^2.1.4",
    "chalk": "^1.1.3",
    "glob": "^7.1.1",
    "lodash": "^4.17.4",
    "remark": "^6.2.0",
    "split-lines": "^1.1.0",
    "temp-write": "^3.0.0",
    "unist-util-visit": "^1.1.1"
  },
  "keywords": [
    "proselint",
    "lint",
    "linting",
    "prose",
    "text",
    "markdown"
  ],
  "devDependencies": {
    "eslint": "^3.15.0",
    "eslint-config-tamia": "^3.3.2",
    "semantic-release": "^6.3.2",
    "semantic-release-tamia": "^1.1.0"
  },
  "release": {
    "analyzeCommits": "semantic-release-tamia/analyzeCommits",
    "generateNotes": "semantic-release-tamia/generateNotes",
    "verifyRelease": "semantic-release-tamia/verifyRelease"
  }
}
